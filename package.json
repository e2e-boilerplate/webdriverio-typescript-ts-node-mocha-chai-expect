{
  "name": "webdriverio-es6modules-typescript-tsnode-mocha-chai-sync",
  "version": "1.0.0",
  "description": "WebdriverIO (sync) end-to-end test automation boilerplate project. Using TypeScript, ES6 modules, ts-node, mocha, and chai",
  "scripts": {
    "test": "npx wdio wdio.conf.js",
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "pretty": "npx prettier --no-config './spec/*.ts' --write"
  },
  "keywords": [
    "webdriverio",
    "typescript",
    "es6 modules",
    "ts-node",
    "mocha",
    "chai",
    "sync",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "@wdio/cli": "^5.15.1",
    "@wdio/dot-reporter": "^5.14.5",
    "@wdio/local-runner": "^5.15.1",
    "@wdio/mocha-framework": "^5.15.1",
    "@wdio/sync": "^5.15.1",
    "chai": "^4.2.0",
    "chromedriver": "^77.0.0",
    "husky": "^3.0.9",
    "mocha": "^6.2.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4",
    "wdio-chromedriver-service": "^5.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
